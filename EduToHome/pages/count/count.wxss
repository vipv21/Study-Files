.c-title{
  width: 100%;
  display: inline-block;
  font-size: 32rpx;
  text-align: center;
  border-top: 1px solid #ccc;
  padding: 40rpx 0;
}
page{
  background-color: #F8F9FD;
}
.count-list{
  width: 100%;
  padding: 28rpx 24rpx;
  height: auto;
  display: inline-block;
  box-sizing: border-box;
  overflow: hidden;
}

.subject-list{
  width: 25%; text-align: center;
  position: relative;
  padding:0 17rpx;
  display:inline-block;
  flex-wrap: wrap; 
  box-sizing: border-box;
  margin-bottom:10px;

}
.subjects{
  width: 120rpx;
  height: 120rpx;
  display: inline-block;
}
.grade{
  width: 60rpx;
  height: 60rpx;
  color: #fff;
  border-radius: 50%;
  /* border: 2px solid #fff; */
  text-align: center;
  line-height: 60rpx;
  position: absolute;
  top:-5rpx;
  right:0rpx;
}
.grade-y{
  background-color: #00CC66;
}
.grade-z{
  background-color: #FF9900;
}
.grade-c{
  background-color: #FF0000;
}
.sub-name{
  padding: 0;
  margin: 0;
  display: inline-block;
  font-size: 28rpx;
  position: relative;
  left: -2rpx;
}









