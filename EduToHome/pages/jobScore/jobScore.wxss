.container{
  width: 100%;
  display: inline-block;
  text-align: center;
  border-top: 1px solid #ccc;
  padding:30rpx;
  box-sizing: border-box;
}
.sTime{
  font-size: 24rpx;
  color: #b4b4b4;
  text-align: left;
  padding: 2rpx 0;
}
.score{
	color: #c8c8c8;
}
.scores{
  font-size: 124rpx;
	color: #ff0000;
}
.containers{
  width: 100%;
  display: inline-block;
  text-align: left;
  border-top: 1px dashed #ccc;
  box-sizing: border-box;
}
.tips{
  margin-top: 5rpx;
  font-size: 24rpx;
  text-align: left;
  padding: 30rpx;
}
.tips-list{
  width: 100%;
  padding-left:15rpx;  
}
.Indexs{
  width: 103rpx;
  font-size: 24rpx;
  display: inline-block;
  margin: 22rpx;
  box-sizing: border-box;
  text-align: center;
}
.tipsIndex{
  display: inline-block;
	width: 90rpx;
	height: 90rpx;
  border-radius: 50%;
	background-color: #4cc366;
	border: solid 1px #a0a0a0;
	font-size: 40rpx;
  color: #fff;
  font-weight: bold;
  text-align: center;
  line-height: 90rpx;

}







